{
    "name": "artemdanilov/flexipic",
    "license": "MIT",
    "description": "Statamic addon for generating responsive images", 
    "autoload": {
        "psr-4": {
            "Artemdanilov\\Flexipic\\": "src"
        }
    },
    "authors": [
        {
            "name": "Artem Danilov"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Flexipic",
            "description": "Generate responsive images with placeholders and lazy loading on the fly"
        },
        "laravel": {
            "providers": [
                "Artemdanilov\\Flexipic\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "bepsvpt/blurhash": "^2.1"
    }
}
